{
  "vimMode": true,
  "spellcheckLanguages": [
    "zh-CN",
    "cy"
  ],
  "spellcheck": false,
  "newFileLocation": "current",
  "attachmentFolderPath": "/asset",
  "promptDelete": false,
  "alwaysUpdateLinks": true,
  "readableLineLength": true,
  "trashOption": "local",
  "pdfExportSettings": {
    "includeName": true,
    "pageSize": "Letter",
    "landscape": false,
    "margin": "0",
    "downscalePercent": 100
  },
  "autoPairMarkdown": false,
  "userIgnoreFilters": null,
  "autoPairBrackets": false,
  "showFrontmatter": true
}